{
  "name": "computercourage/proto",
  "description": "A WordPress Framework That Works",
  "type": "library",
  "repositories":[
    {
      "type": "composer",
      "url": "https://pivvenit.github.io/acf-composer-bridge/composer/v3/wordpress-plugin/"
    },
    {
      "type":"composer",
      "url":"https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    }
  ],
  "require": {
    "wp-packagist/wp-packagist": "^1.0",
    "wpackagist-plugin/timber-library": "*",
    "wpackagist-plugin/wordpress-seo": "*",
    "wpackagist-plugin/wp-cfm": "*",
    "wpackagist-plugin/acf-content-analysis-for-yoast-seo": "*",
    "advanced-custom-fields/advanced-custom-fields-pro" : "^5.9.5"
  },
  "extra": {
    "installer-paths": {
      "wp-content/plugins/{$name}/": ["type:wordpress-plugin"]
    }
  },
}
